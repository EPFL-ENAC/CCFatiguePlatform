{
  "packages": {
    ".": {
      "component": "root",
      "release-type": "node",
      "changelog-path": "CHANGELOG.md",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false
    },
    "backend": {
      "component": "backend",
      "release-type": "python"
    },
    "frontend": {
      "component": "frontend",
      "release-type": "node"
    }
  },
  "plugins": [
    {
      "type": "linked-versions",
      "group-name": "ccfatigue",
      "components": ["root", "backend", "frontend"]
    }
  ]
}
